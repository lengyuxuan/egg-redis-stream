{
  "eggPlugin": {
    "name": "redisStream",
    "dependencies": [
      "redis"
    ]
  },
  "egg": {
    "framework": "egg",
    "tsHelper": {
      "generatorConfig": {
        "producer": {
          "declareTo": "Application.producer",
          "directory": "app/producer",
          "enabled": true,
          "generator": "class",
          "interface": "IProducer"
        },
        "consumer": {
          "declareTo": "Application.consumer",
          "directory": "app/consumer",
          "enabled": true,
          "generator": "class",
          "interface": "IConsumer"
        }
      }
    }
  },
  "types": "./index.d.ts",
  "main": "app.js",
  "name": "egg-redis-stream",
  "version": "0.0.3",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "p": "tsc -p . && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lengyuxuan/egg-redis-stream.git"
  },
  "bugs": {
    "url": "https://github.com/lengyuxuan/egg-redis-stream/issues"
  },
  "homepage": "https://github.com/lengyuxuan/egg-redis-stream#readme",
  "author": "FangJin",
  "license": "ISC",
  "description": "redis stream message queue for egg.js",
  "tap": {
    "ts": true
  },
  "dependencies": {
    "@types/node": "^20.5.0"
  },
  "devDependencies": {
    "@eggjs/tsconfig": "^1.3.3",
    "egg": "^3.17.4",
    "egg-redis": "^2.5.0"
  }
}
